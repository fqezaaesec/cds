{
  "name": "@coinbase/cds-vscode-plugin",
  "version": "0.0.0",
  "private": true,
  "displayName": "@coinbase/cds-vscode-plugin",
  "description": "VSCode Plugin for building with the Coinbase Design System",
  "repository": {
    "type": "git",
    "url": "git@github.com:coinbase/cds.git",
    "directory": "packages/vscode-plugin"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "watch": "rm -rf dist && rollup -c -w"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:javascript",
    "onLanguage:css",
    "onLanguage:scss"
  ],
  "type": "commonjs",
  "main": "./cjs/client.js",
  "files": [
    "cjs",
    "dts",
    "CHANGELOG"
  ],
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/vscode": "^1.92",
    "rollup": "^4.22.4",
    "tslib": "^2",
    "typescript": "~5.9.2"
  }
}
